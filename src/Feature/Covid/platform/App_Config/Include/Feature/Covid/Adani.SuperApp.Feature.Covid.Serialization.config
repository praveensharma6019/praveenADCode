<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
	<sitecore role:require="Standalone or ContentManagement">
		<unicorn>
			<configurations>
				<configuration name="Feature.Covid" description="Covid Component Content" dependencies="Adani.SuperApp.Airport.Foundation.Serialization">
					<targetDataStore physicalRootPath="$(sourceFolder)\$(layer)\$(module)\serialization" useDataCache="false" singleInstance="true"/>
					<predicate>
						
						<include name="Rendering.Feature.Covid" database="master" path="/sitecore/layout/Renderings/Feature/Adani/Covid" />
						<include name="Template.Feature.Covid" database="master" path="/sitecore/templates/Feature/Adani/AirportCommon/Covid" />
						<include name="Template.Feature.CovidCarousal" database="master" path="/sitecore/templates/Feature/Adani/AirportCommon/Covid Crausal" />
						<include name="LayoutService.Feature.Covid" database="master" path="/sitecore/system/Modules/Layout Service/Rendering Contents Resolvers/Adani/Common/Covid Content Resolver"/>
					</predicate>
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>