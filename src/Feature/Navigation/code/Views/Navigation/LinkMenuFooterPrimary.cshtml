@using Sitecore.Feature.Navigation
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Foundation.Dictionary.Extensions
@model Sitecore.Feature.Navigation.Models.NavigationItems

@{
	if (Model?.NavItems == null)
	{
		return;
	}

	var count = 2;
	var clmClass = "";
}

<div class="footerMenu txt-right">
    <ul>
        @foreach (var item in Model.NavItems)
        {

            if (!item.IsNewTab)
            {               
                <li><a href="@item.Url" target="@item.Target">@Html.Sitecore().Field(Templates.Navigable.Fields.NavigationTitle, item.Item)</a></li>
            }
            else
            {               
                <li><a href="@item.Url" target="_blank">@Html.Sitecore().Field(Templates.Navigable.Fields.NavigationTitle, item.Item)</a></li>
            }
        }
    </ul>
</div>
