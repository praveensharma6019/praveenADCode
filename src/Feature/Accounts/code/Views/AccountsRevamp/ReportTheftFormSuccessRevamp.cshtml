@{
    Sitecore.Data.Items.Item successPage = Sitecore.Context.Database.GetItem("{AD8E855C-207B-4B11-9D2B-3F92FD8B2856}");
    string callbackPageUrl = Sitecore.Links.LinkManager.GetItemUrl(successPage);
}

<input type="hidden" value="@callbackPageUrl" id="callbackPageUrl" />
<script type="text/javascript">
    $(document).ready(function () {
        var callbackPageUrl = $('#callbackPageUrl').val();
        location.href = callbackPageUrl + '?IsSuccess=1';
    })
</script>