@using Sitecore.Foundation.Dictionary.Extensions
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Configuration
@using CaptchaMvc;
@using CaptchaMvc.HtmlHelpers;
@model Sitecore.Feature.Accounts.Models.ChangeOfNameApplicationFromModel
<style>
    .disabled {
        pointer-events: none;
    }
</style>
<div class="collapsible-header hide center">
    <a href="javascript:void(0)">Existing Account Details</a>
</div>
<div class="collapsible-body">
    <h2>Existing Account Details</h2>
    @using (Html.BeginRouteForm(MvcSettings.SitecoreRouteName, FormMethod.Post, new { id = "frmCONApplication", @class = "form-signin", enctype = "multipart/form-data" }))
    {
        @Html.AddUniqueFormId()
        @Html.HiddenFor(m => m.Aklasse)
        @Html.HiddenFor(m => m.IsLEC)
        @Html.HiddenFor(m => m.AccountNo)
        @Html.HiddenFor(m => m.MeterNumber)
        @Html.HiddenFor(m => m.ConnectionType)
        @Html.HiddenFor(m => m.ExistingMobileNumber)
        @Html.HiddenFor(m => m.ExistingEmailId)
        @Html.HiddenFor(m => m.MobileNo)
        <input type="hidden" value="@Model.formActiveId" name="formActiveId" id="formActiveId" />
        <div class="existing-acc-detail section-wrapper">
            <div class="card brand-gradient outlined">
                <ul>
                    <li>
                        <span>Account Number</span> @Model.AccountNo
                    </li>
                    <li>
                        <span>Meter Number</span>  @Model.MeterNumber
                    </li>
                    <li>
                        <span>Connection Type</span>@Model.ConnectionType
                    </li>
                    <li>
                        <span>Registered Customer Name</span>  @Model.ConsumerName
                    </li>
                    <li>
                        <span>Mobile Number</span>  @Model.ExistingMobileNumber
                    </li>
                    <li>
                        <span>Email ID</span> @Model.ExistingEmailId
                    </li>
                </ul>
            </div>
        </div>
        <h2>Submit your name change request in 3 simple steps:</h2>
        <div class="step-progress-bar stepper-wrapper">
            <div class="section-wrapper">
                <ul class="stepper-header" id="stepper-holder" data-stepper-content-wrapper="stepperContent" data-stepper-content-holder="stepper-content-body">
                    <li class="complete stepper-action active-step" data-id="1" data-content-target="content1">
                        1. Premise
                        <span></span>
                    </li>
                    <li class="stepper-action" data-id="2" data-content-target="content2">
                        2. Details
                        <span></span>
                    </li>
                    <li class="stepper-action" data-id="3" data-content-target="content3">
                        3. Documents
                        <span></span>
                    </li>
                </ul>
            </div>
        </div>
        <div class="stepper-content" id="stepperContent">
            <div class="active stepper-content-body" id="content1">
                <div class="new-connection-form">
                    <div class="step-1-form section-wrapper">
                        <div class="card outlined card-with-form">
                            <div class="card-header">
                                <h6>Select Premise</h6>
                                <a href="#generalguideline" class="waves-effect modal-trigger">Check Guidelines</a>
                            </div>
                            <div class="card-body">
                                <div class="form-field">
                                    <div class="input-field custom">
                                        @Html.DropDownListFor(m => m.SelectedPremiseType, new SelectList(Model.PremiseTypeSelectList, "Value", "Text", Model.SelectedPremiseType), "Select", new
                                        {
                                            @class = "form-control",
                                            id = "ddlPremiseType"@*, required = "required"*@})
                                        <label>Select Applicable Premise</label>
                                        <p class="field-validation-error" id="ddlPremiseType" style="display:none;"></p>
                                        @*@Html.ValidationMessageFor(m => m.SelectedPremiseType)*@
                                    </div>
                                </div>
                                <div class="btn-group" id="sub">
                                    <button type="button" class="waves-effect waves-light btn btn-continue next-step continueSecondStep" @*onclick="goToStepNo('stepper-holder', 2);"*@>Continue</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="stepper-content-body hide" id="content2">
                <div class="new-connection-form">
                    @if (Model.SelectedPremiseType != null)
                    {
                        <div class="step-2-form section-wrapper">
                            <div class="card outlined card-with-form">
                                <div class="card-header">
                                    <h6>Enter Details</h6>
                                </div>
                                <div class="card-body">
                                    <div class="form-field">
                                        <h6>Applicant Type</h6>
                                        <div class="field-group">
                                            @if (Model.SelectedPremiseType == "009")
                                            {
                                                <div class="radio-field">
                                                    <label for="ApplicantTypeIndividual">
                                                        @Html.RadioButtonFor(model => model.ApplicantType, "1", new { id = "ApplicantTypeIndividual", @class = "with-gap rbApplicantType", @checked = "checked" })
                                                        <span>Individual</span>
                                                    </label>
                                                </div>
                                            }
                                            else
                                            {
                                                <div class="radio-field">
                                                    <label for="ApplicantTypeIndividual">
                                                        @Html.RadioButtonFor(model => model.ApplicantType, "1", new { id = "ApplicantTypeIndividual", @class = "with-gap rbApplicantType" })
                                                        <span>Individual</span>
                                                    </label>
                                                </div>
                                            }

                                            @if (Model.SelectedPremiseType != "009")
                                            {
                                                <div class="radio-field">
                                                    <label for="ApplicantTypeJointOccupancy">
                                                        @Html.RadioButtonFor(model => model.ApplicantType, "3", new { id = "ApplicantTypeJointOccupancy", @class = "with-gap rbApplicantType" })
                                                        <span>Joint Occupancy (Single Applicant)</span>
                                                    </label>
                                                </div>
                                                <div class="radio-field">
                                                    <label for="ApplicantTypeJoint">
                                                        @Html.RadioButtonFor(model => model.ApplicantType, "2", new { id = "ApplicantTypeJoint", @class = "with-gap rbApplicantType" })
                                                        <span>Joint</span>
                                                    </label>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                    @if (Model.SelectedPremiseType != "009" && Model.SelectedPremiseType != "028" && Model.SelectedPremiseType != "034" && Model.SelectedPremiseType != "036")
                                    {
                                        <div class="form-field">
                                            <h6>Registered customer still living</h6>
                                            <div class="field-group">
                                                <div class="radio-field">
                                                    <label for="ApplicantLivingYes">
                                                        @Html.RadioButtonFor(model => model.IsStillLiving, "Yes", new { id = "ApplicantLivingYes", @class = "with-gap rbIsStillLiving" })
                                                        <span>Yes</span>
                                                    </label>
                                                </div>
                                                <div class="radio-field">
                                                    <label for="ApplicantLivingNo">
                                                        @Html.RadioButtonFor(model => model.IsStillLiving, "No", new { id = "ApplicantLivingNo", @class = "with-gap rbIsStillLiving" })
                                                        <span>No</span>
                                                    </label>
                                                </div>
                                                @Html.ValidationMessageFor(x => x.IsStillLiving, "", new { @class = "help-block" }, "p")
                                            </div>
                                        </div>
                                    }
                                    <div class="form-field">
                                        <h6>Applicant Details</h6>
                                        @if (Model.ApplicantType == "2")
                                        {

                                            <div class="row field-flex">
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.Name1Joint, new { @maxlength = "50" })
                                                    <label for="">Full Name (Applicant 1)<span class="required">*</span></label>
                                                    <p class="field-validation-error" id="Name1Joint" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.Name2Joint, new { @maxlength = "50" })
                                                    <label for="">Full Name (Applicant 2)<span class="required">*</span></label>
                                                    <p class="field-validation-error" id="Name2Joint" style="display:none;"></p>
                                                </div>
                                            </div>

                                        }
                                        else if (Model.SelectedTitle == "0006")
                                        {
                                            <div class="row field-flex">
                                                <div class="input-field custom col s12 m4">
                                                    @Html.DropDownListFor(m => m.SelectedTitle, new SelectList(Model.TitleSelectList, "Value", "Text", "0006"), "Select", new
                                                    {
                                                        id = "ddlTitle"
                                               })
                                                    <label>Select Title <span class="required">*</span></label>
                                                    @Html.ValidationMessageFor(x => x.TitleSelectList)
                                                    <p class="field-validation-error" id="ddlTitle" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m8">
                                                    @Html.TextBoxFor(x => x.OrganizationName, new
                                                    {@*required = "required",*@@maxlength = "50"
})
                                                    <label for="">Name of Organization<span class="required">*</span></label>
                                                    <p class="field-validation-error" id="OrganizationName" style="display:none;"></p>
                                                </div>
                                            </div>
                                        }
                                        else
                                        {
                                            <div class="row field-flex">
                                                <div class="input-field custom col s12 m4">
                                                    @Html.DropDownListFor(m => m.SelectedTitle, new SelectList(Model.TitleSelectList, "Value", "Text", Model.SelectedTitle), "Select", new
                                                    {
                                                        id = "ddlTitle"@*, required = "required"*@})
                                                    <label>Select Title <span class="required">*</span></label>
                                                    @Html.ValidationMessageFor(x => x.TitleSelectList)
                                                    <p class="field-validation-error" id="ddlTitle" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m8">
                                                    @Html.TextBoxFor(x => x.FirstName, new
                                                    {@*required = "required",*@@maxlength = "50"
})
                                                    <label for="">First Name <span class="required">*</span></label>
                                                    <p class="field-validation-error" id="FirstName" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.MiddleName, new { @maxlength = "50" })
                                                    <label for="">Middle Name</label>
                                                    <p class="field-validation-error" id="MiddleName" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.LastName, new
                                                    {@*required = "required",*@@maxlength = "50"
})
                                                    <label for="">Last Name <span class="required">*</span></label>
                                                    <p class="field-validation-error" id="LastName" style="display:none;"></p>
                                                </div>
                                            </div>
                                        }

                                    </div>
                                    @if (Model.SelectedPremiseType == "00026")  //to skip this step added 00
                                    {
                                        <div class="form-field">
                                            <h6>Address Correction required</h6>
                                            <div class="field-group">
                                                <div class="radio-field">
                                                    <label for="IsAddressCorrectionRequiredNo">
                                                        @Html.RadioButtonFor(model => model.IsAddressCorrectionRequired, "No", new { id = "IsAddressCorrectionRequiredNo", @class = "with-gap" })
                                                        <span>No</span>
                                                    </label>
                                                </div>
                                                <div class="radio-field">
                                                    <label for="IsAddressCorrectionRequiredYes">
                                                        @Html.RadioButtonFor(model => model.IsAddressCorrectionRequired, "Yes", new { id = "IsAddressCorrectionRequiredYes", @class = "with-gap" })
                                                        <span>Yes</span>
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        if (Model.IsAddressCorrectionRequired == "Yes")
                                        {
                                            <div class="form-field">
                                                <h6>Applicant's Present / Correspondence Address :</h6>
                                                <div class="row field-flex">
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.TextBoxFor(x => x.HouseNumber, new
                                                        {
                                                            @maxlength = "10"@*, required = "required"*@})

                                                        <label for="">House Number <span class="required">*</span></label>
                                                        <p class="field-validation-error" id="HouseNumber" style="display:none;"></p>
                                                    </div>
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.TextBoxFor(x => x.Street, new
                                                        {
                                                            @maxlength = "30"@*, required = "required"*@})

                                                        <label for="">Street <span class="required">*</span></label>
                                                        <p class="field-validation-error" id="Street" style="display:none;"></p>
                                                    </div>
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.TextBoxFor(x => x.Landmark, new
                                                        {
                                                            @maxlength = "30"@*, required = "required"*@})

                                                        <label for="">Landmark <span class="required">*</span></label>
                                                        @Html.ValidationMessageFor(x => x.Landmark, "", new { @class = "help-block" }, "p")
                                                        <p class="field-validation-error" id="Landmark" style="display:none;"></p>
                                                    </div>
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.TextBoxFor(x => x.Area, new
                                                        {
                                                            @maxlength = "50"@*, required = "required"*@})

                                                        <label for="">Area <span class="required">*</span></label>
                                                        <p class="field-validation-error" id="Area" style="display:none;"></p>
                                                    </div>
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.DropDownListFor(m => m.SelectedSuburb, new SelectList(Model.SuburbSelectList, "Value", "Text"), "Select", new
                                                        {@*required = "required"*@})
                                                        <label for="">Suburb <span class="required">*</span></label>
                                                        <p class="field-validation-error" id="ddlSelectedSuburb" style="display:none;"></p>
                                                    </div>
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.DropDownListFor(m => m.SelectedCity, new SelectList(Model.CitySelectList, "Value", "Text"), "Select", new
                                                        {@*required = "required"*@})
                                                        <label for="">City <span class="required">*</span></label>
                                                        <p class="field-validation-error" id="ddlSelectedCity" style="display:none;"></p>
                                                    </div>
                                                    <div class="input-field custom col s12 m6">
                                                        @Html.DropDownListFor(m => m.SelectedPincode, new SelectList(Model.PincodeSelectList, "Value", "Text"), "Select", new
                                                        {@*required = "required"*@})
                                                        <label for="">Pincode <span class="required">*</span></label>
                                                        <p class="field-validation-error" id="ddlSelectedPincode" style="display:none;"></p>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    }
                                    <div class="form-field">
                                        <h6>Contact Details</h6>
                                        <div class="row">
                                            <div class="input-field custom col s12 m6">
                                                @Html.TextBoxFor(x => x.EmailId, new
                                                {@*required = "required",*@@maxlength = "50"
})
                                                <label for="">Email <span class="required">*</span></label>
                                                <p class="field-validation-error" id="EmailId" style="display:none;"></p>
                                            </div>
                                            <div class="input-field custom col s12 m6">
                                                @Html.TextBoxFor(x => x.Landline)
                                                <label for="">Landline Number</label>
                                                <p class="field-validation-error" id="Landline" style="display:none;"></p>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="input-field custom col s12 m6">
                                                @Html.TextBoxFor(x => x.MobileNo, new
                                           { @readonly = "readonly" })
                                                <label for="">Mobile Number <span class="required">*</span></label>
                                                <p class="field-validation-error" id="MobileNo" style="display:none;"></p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-field">
                                        <h6>Billing Details</h6>
                                        <div class="row">
                                            <div class="input-field custom col s12 m6">
                                                @Html.DropDownListFor(m => m.SelectedBillLanguage, new SelectList(Model.BillLanguageSelectList, "Value", "Text", Model.SelectedBillLanguage), "Select", new
                                                {@*required = "required"*@})
                                                <label>Bill Language <span class="required">*</span></label>
                                                <p class="field-validation-error" id="SelectedBillLanguage" style="display:none;"></p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-field">
                                        <h6>Bill Format :</h6>
                                        <div class="field-group">
                                            <div class="radio-field">
                                                <label class="form-check-label" for="IsPaperlessNo">
                                                    @Html.RadioButtonFor(model => model.IsPaperlessBilling, "No", new
                                                    {
                                                        id = "IsPaperlessNo"@*, required = "required"*@,
@class = "with-gap"
})
                                                    <span>   E-Bill + Hard Copy of Bill </span>
                                                </label>
                                            </div>
                                            <div class="radio-field">
                                                <label class="form-check-label" for="IsPaperlessYes">
                                                    @Html.RadioButtonFor(model => model.IsPaperlessBilling, "Yes", new
                                                    {
                                                        id = "IsPaperlessYes"@*, required = "required"*@,
@class = "with-gap"
})
                                                    <span> Paperless ** </span>
                                                </label>
                                            </div>
                                        </div>
                                        <div class="message warning warning-info" style="font-size:12px;">
                                            <aside>
                                                <i class="i-info"></i>
                                            </aside>
                                            <p>
                                                ** Enjoy ₹10 off on your electricity bill every month
                                            </p>
                                        </div>
                                    </div>
                                    @if (Model.SelectedPremiseType == "034")
                                    {
                                        <div class="form-field">
                                            <h6>Applicant's Present / Correspondence Address :</h6>
                                            <div class="row field-flex">
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.LandlordName, new
                                                    {
                                                        @maxlength = "20"@*, required = "required"*@})

                                                    <label for="">Name of Owner <span class="required">*</span></label>
                                                    <p class="field-validation-error" id="LandlordName" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.LandlordMobile, new
                                                    {
                                                        @maxlength = "10"@*, required = "required"*@})

                                                    <label for="">Mobile No <span class="required">*</span></label>
                                                    <p class="field-validation-error" id="LandlordMobile" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.LandlordLandline, new { @maxlength = "11" })
                                                    <label for="">Landline No</label>
                                                    <p class="field-validation-error" id="LandlordLandline" style="display:none;"></p>
                                                </div>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.LandlordEmail, new { @maxlength = "30" })
                                                    <label for="">Email</label>
                                                    <p class="field-validation-error" id="LandlordEmail" style="display:none;"></p>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    <div class="form-field">
                                        <h6>Do you wish to transfer the Security Deposit from the exisiting account?</h6>
                                        <div class="field-group">
                                            <div class="radio-field">
                                                <label class="form-check-label" for="IsContinueWithExistingSDNo">
                                                    @Html.RadioButtonFor(model => model.IsContinueWithExistingSD, "No", new
                                                    {
                                                        id = "IsContinueWithExistingSDNo"@*, required = "required"*@,
@class = "with-gap rbIsContinueWithExistingSD"
})
                                                    <span>No</span>
                                                </label>
                                            </div>
                                            <div class="radio-field">
                                                <label class="form-check-label" for="IsContinueWithExistingSDYes">
                                                    @Html.RadioButtonFor(model => model.IsContinueWithExistingSD, "Yes", new
                                                    {
                                                        id = "IsContinueWithExistingSDYes"@*, required = "required"*@,
@class = "with-gap rbIsContinueWithExistingSD"
})
                                                    <span>Yes</span>
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                    @if (Model.IsContinueWithExistingSD == "Yes")
                                    {
                                        <div class="form-field">
                                            <div class="row">
                                                <input type="hidden" value="@Model.ExistingSecurityDepositeAmount" id="hdnExistingSecurityDepositeAmount" />
                                                <h6>Security Deposit Amount in the exisiting account is: Rs. @Model.ExistingSecurityDepositeAmount</h6>
                                                <div class="input-field custom col s12 m6">
                                                    @Html.TextBoxFor(x => x.SecurityDepositeAmount, new
                                                    {
                                                        @class = "form-control"@*, required = "required"*@})

                                                    <label for="">Security Deposit Amount</label>
                                                    @Html.ValidationMessageFor(x => x.SecurityDepositeAmount, "", new { @class = "help-block" }, "p")
                                                </div>
                                            </div>
                                        </div>
                                        <div class="message warning warning-info" style="font-size:14px;">
                                            <p class="txt-sm incentiveMsg">Please Note: The amount mentioned above shall be transferred & maintained as a “Security Deposit” in the new account, once the Change of Name request is processed. Additional security deposit, if applicable, shall be added in the 1st bill generated for the new account.</p>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="message warning warning-info" style="font-size:14px;">
                                            <div class="cms-content">
                                                Please Note:
                                                <ul>
                                                    <li>The security deposit payable for the new account shall be calculated once the processing of the “Change of Name” request is complete. The calculated amount shall be included in the 1st bill generated for the new account.</li>
                                                    <li>As per MERC Regulations-2021 & clauses under 13.1 and 13.2 of the said Regulations all electricity consumers in the state are now required to maintain a Security Deposit with their electricity distribution license, equivalent to twice the average of last 12 months Bill amount (Recorded in the period of April to Mar of immediately Preceding FY).</li>
                                                </ul>
                                            </div>
                                        </div>
                                    }
                                    <div class="input-field custom">
                                        @Html.TextAreaFor(x => x.ConsumerRemark, new { @class = "materialize-textarea", @maxlength = "800", id = "remark" })
                                        <label for="remark">Enter Your Remarks</label>
                                        <p class="field-validation-error" id="ConsumerRemark" style="display:none;"></p>
                                    </div>
                                    <div class="btn-group">
                                        <button type="button" class="waves-effect waves-light btn-flat backSecondStep">Back</button>
                                        <button type="button" class="waves-effect waves-light btn btn-continue continueThirdStep">
                                            Continue
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
            <div class="stepper-content-body hide" id="content3">
                <div class="new-connection-form">
                    @if (Model.SelectedPremiseType != null)
                    {
                        <div class="step-3-form section-wrapper">
                            <div class="card outlined card-with-form">
                                <div class="card-header">
                                    <h6>Upload Documents</h6>
                                    <a href="#docsupload" class="waves-effect modal-trigger">Check Guidelines</a>
                                </div>
                                <div class="card-body">
                                    <div class="form-field">
                                        @{
                                            var ExistingIDDocument = Model.GetExistingDocuments != null ? Model.GetExistingDocuments.Where(x => x.DocumentTypeCode.Trim() == "ID").ToList() : null;
                                        }
                                        @if (Model.IDDocumentsList.Count > 0 || Model.IDDocumentsListOnly1.Count > 0)
                                        {
                                            <h6>Proof Of Identity Document</h6>
                                            {
                                                var i1 = 1;
                                                if (Model.IDDocumentsList.Count > 0)
                                                {
                                                    foreach (var doc in Model.IDDocumentsList)
                                                    {
                                                        var IDDocument = ExistingIDDocument.Where(d => d.DocumentType == doc.DocType).FirstOrDefault();
                                                        var fileupload1 = "file_" + System.Convert.ToString(doc.DocType);
                                                        var downloadDoc = "download_ID";
                                                        var docname = "docnumber_" + System.Convert.ToString(doc.DocType);
                                                        if (doc.IsMandatory)
                                                        {
                                                            var mad = "mand_" + doc.DocType;
                                                            @Html.HiddenFor(m => doc.IsMandatory, new { id = mad })
                                                        }
                                                        var madname = "manddocname_" + System.Convert.ToString(doc.DocType);
                                                        @Html.HiddenFor(m => doc.DocName, new { id = madname })
                                                        <div class="row field-flex">
                                                            <div class="input-field custom col s12 m4">
                                                                @doc.DocName
