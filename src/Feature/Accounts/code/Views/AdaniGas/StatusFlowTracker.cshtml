                                                @Html.TextBoxFor(x => x.EmailId, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.EmailId)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                                <div class="mt-3">
                                    <label for="">Applicant Details <span class="required"></span></label>

                                    <div class="row mt-2">

                                        <div class="col-lg-4 col-md-4">
                                            <div class="form-group">
                                                <label for="">DATE <span class="required">*</span></label>
                                                @Html.TextBoxFor(x => x.CreatedDate, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.CreatedDate)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-4">
                                            <div class="form-group">
                                                <label for="">TRANSACTION NUMBER	 <span class="required">*</span></label>
                                                @Html.TextBoxFor(x => x.RegisterdConsumerName, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.RegisterdConsumerName)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-4">
                                            <div class="form-group">
                                                <label for="">AMOUNT PAID <span class="required">*</span></label>
                                                @Html.TextBoxFor(x => x.ConnectionType, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.ConnectionType)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-4">
                                            <div class="form-group">
                                                <label for="">ADDRESS <span class="required">*</span></label>
                                                @Html.TextBoxFor(x => x.CustomerAddress, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.CustomerAddress)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-4">
                                            <div class="form-group">
                                                <label for="">Application Type <span class="required">*</span></label>
                                                @Html.TextBoxFor(x => x.ApplicationType, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.ApplicationType)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                        <div class="col-lg-4 col-md-4">
                                            <div class="form-group">
                                                <label for="">SOCIETY TYPE <span class="required">*</span></label>
                                                @Html.TextBoxFor(x => x.SocietyType, new { @class = "form-control reset-control", id = "FirstName", @readonly = "readonly" })
                                                @Html.HiddenFor(x => x.SocietyType)
                                                <small id="" class="form-text text-muted"></small>
                                            </div>
                                        </div>

                                    </div>
                                </div>


                                <div class="tab-pane" id="Documents" role="tabpanel" aria-labelledby="Documents-tab">
                                    <div class="mt-3">
                                        <b><label class="form-label" for="customFile">Uploaded Document<span class="required"></span></label></b>
                                    </div>
                                    @if (Model.ApplicationType != null && Model.ApplicationType == "Builder_Case")
                                    {
                                        <div class="row buildercase">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <div class="mt-2">
                                                        <label class="form-label" for="customFile">Index (or) Sales Deed<span class="required">*</span></label>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.IndexBuilderId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row buildercase">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label" for="customFile">Possession Letter from Builder<span class="required">*</span></label>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.PossessionletterBuilderId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row buildercase">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label" for="customFile">Photo ID Proof of Owner<span class="required">*</span></label>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.PhotoIDBuilderId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row buildercase">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <label class="form-label" for="customFile">Co-owner consent with signed photo ID<span class="required">*</span></label>
                                                </div>
                                            </div>
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.CoownerBuilderId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>

                                    }
                                    @if (Model.ApplicationType != null && Model.ApplicationType == "Demise")
                                    {
                                        <div class="row demise">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">

                                                    <div class="mt-2">
                                                        <input type="text" class="form-control" value="Death Certificate of Registered customer" readonly="">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.DeathCertificateDemiseId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row demise">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">

                                                    <div class="mt-2">
                                                        <input type="text" class="form-control" value="Documentary proof of natural succession like succession certificate / Probate of will (Originals for verification/affidavit swearing succession cum NOC from other legal heir other than the spouse of registered customer)" readonly="">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.DocumentaryDemiseId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    @if (Model.ApplicationType != null && Model.SocietyType != null && Model.SocietyType == "Registered Housing Society")
                                    {
                                        <div class="row RegisteredHousing">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">

                                                    <div class="mt-2">
                                                        <input type="text" class="form-control" value="Agreement / Purchase / Sale deed with the present registered customers (or) Share certificate of Registered Society" readonly="">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.FirstRegisteredHousingSocietypropertyresaleId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row RegisteredHousing">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">

                                                    <div class="mt-2">
                                                        <input type="text" class="form-control" value="Latest Municipal tax bill in applicant's name. (or) NOC (Duly stamped - No Objection Certificate) from the registered housing society" readonly="">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.SecondRegisteredHousingSocietypropertyresaleId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                    @if (Model.ApplicationType != null && Model.SocietyType != null && Model.SocietyType == "Unregistered Housing Society")
                                    {
                                        <div class="row UnregisteredHousing">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">

                                                    <div class="mt-2">
                                                        <input type="text" class="form-control" value="Agreement / Purchase / Sale deed with the present registered customer" readonly="">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.FirstUnregisteredHousingSocietypropertyresaleId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row UnregisteredHousing">
                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">

                                                    <div class="mt-2">
                                                        <input type="text" class="form-control" value="No Objection Certificate (NOC) from the present registered customer (or) Latest Municipal tax bill." readonly="">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-lg-6 col-md-6">
                                                <div class="form-group">
                                                    <a class="txt-orange otherdocumentfile" href="@Url.Action("DownloadNameTransferAttachmentFile", new { id =  Model.SecondUnregisteredHousingSocietypropertyresaleId })">
                                                        <i class="fa fa-download mr-2"></i> Download Uploaded Document
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>

                                <div>
                                    <button type="button" class="btn btn-primary w-auto" name="ApproveNameTrnasfer" value="Approve" id="Approve_By_Admin">Approve</button>

                                    <button type="button" class="btn btn-primary w-auto" name="RejectNameTrnasfer" value="Reject" id="Reject_By_Admin">Reject</button>

                                    <button type="button" class="btn btn-primary w-auto" name="UpdateNameTrnasfer" value="Ask for additional Details / Documents" id="Additional_Details_By_Admin">Ask for additional Details / Documents</button>
                                </div>

                            </div>
                        </div>
                    </section>
                }
            }
        </div>
    </div>
</section>

<div id="loader-wrapper" style="display:none">
    <div id="loader"></div>
    <div class="loader-content" style="text-align:center;"><p><b>Your request is being processed. Please do not press the “Back” or “Refresh” button.</b></p></div>
    <div class="loader-section section-left"></div>
    <div class="loader-section section-right"></div>
</div>

<div id="confirmation_modal" class="modal fade show" role="dialog" style="display: none; padding-right: 17px;" data-id="">
    <div class="modal-dialog modal-dialog-centered">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <p>Message</p>
                <button type="button" class="close reset" data-dismiss="modal">X</button>
            </div>
            <div class="modal-body">
                <p class="confirmation_modal_message"></p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary confirmation_modalbtnYesNO" value="1">Proceed</button>
                <button type="button" class="btn btn-primary confirmation_modalbtnYesNO" value="0">Cancel</button>
            </div>
        </div>
    </div>
</div>

<div id="message_modal" class="modal fade show" role="dialog" style="display: none; padding-right: 17px;" data-id="">
    <div class="modal-dialog modal-dialog-centered">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <p>Message</p>
                <button type="button" class="close" data-dismiss="modal">X</button>
            </div>
            <div class="modal-body">
                <p class="message_modal_message"></p>
            </div>
            <button type="button" style="padding-bottom:10px;" class="close" data-dismiss="modal">Ok</button>
        </div>
    </div>
</div>

<div id="submit_message_modal" class="modal fade show" role="dialog" style="display: none; padding-right: 17px;" data-id="">
    <div class="modal-dialog modal-dialog-centered">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <p>Message</p>
                <button type="button" class="close" data-dismiss="modal">X</button>
            </div>
            <div class="modal-body">
                @if (Session["Message"] != null && !string.IsNullOrEmpty(Session["Message"].ToString()))
                {
                    <div class="form-group row">
                        <label for="" class="col-sm-12 col-form-label txt-green">@Session["Message"].ToString()</label>
                        @*@if (Session["AlertMessage"] != null && !string.IsNullOrEmpty(Session["AlertMessage"].ToString()))
                            {
                                <p class="col-sm-12 col-form-label txt-orange">@Session["AlertMessage"].ToString()</p>
                            }*@
                    </div>
                    Session["Message"] = null;
                    Session["AlertMessage"] = null;
                }
            </div>
            <button type="button" style="padding-bottom:10px;" class="close" data-dismiss="modal">Ok</button>
        </div>

    </div>
</div>

