                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <div class="form-group">
                                                            <label for="">Address</label>
                                                            @Html.TextBoxFor(x => x.CustomerAddress, new { @class = "form-control reset-control", id = "CustomerAddress", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6 mb-3">
                                                        <div class="form-group">
                                                            <label for="">Outstanding Amount</label>
                                                            @Html.TextBoxFor(x => x.OutstandingAmount, new { @class = "form-control reset-control", id = "Amount", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                }
                                                <div class="col-lg-12">
                                                    @Html.Partial("~/Views/AdaniGas/Partial/_SubmitYourRequestForNameTransfer.cshtml", Model)
                                                </div>
                                            }
                                            @if (Model.stepTwo == true)
                                            {
                                                <div>
                                                    @Html.Partial("~/Views/AdaniGas/Partial/_QuickPayAdaniGasNameTransfer.cshtml", Model)
