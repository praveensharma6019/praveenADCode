                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Customer ID <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.CustomerID, new { @class = "form-control reset-control", id = "Customer ID", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            @Html.ValidationMessageFor(x => x.CustomerID, "", new { @class = "help-block" }, "p")
                                                            @Html.HiddenFor(x => x.CustomerID)
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Meter Number <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.MeterNumber, new { @class = "form-control reset-control", id = "CustomerName", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Customer ID </label>
                                                            @Html.TextBoxFor(x => x.CustomerID, new { @class = "form-control reset-control", id = "Customer ID", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Meter Number </label>
                                                            @Html.TextBoxFor(x => x.MeterNumber, new { @class = "form-control reset-control", id = "CustomerName", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Mobile Number</label>
                                                            @Html.TextBoxFor(x => x.MobileNumber, new { @class = "form-control", id = "MobileNumber", maxlength = "10", @disabled = "disabled" })
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6" style="display:none">
                                                        <div class="form-group">
                                                            <label for="">Email Id</label>
                                                            @Html.TextBoxFor(x => x.CustomerEmailId, new { @class = "form-control", id = "CustomerEmailId", @disabled = "disabled" })
                                                        </div>
                                                    </div>
                                                }
                                            }
                                            else
                                            {
                                                if (Model.ProceedWith == "1")
                                                {
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@*@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Customer Id", "Enter Customer Id or Meter Number")*@Enter Customer Id <b>or</b> Meter Number <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.InputByUser, new { @class = "form-control", id = "InputByUser", maxlength = "25", required = "required", @autocomplete = "off" })
                                                            @Html.ValidationMessageFor(x => x.InputByUser, "", new { @class = "help-block" }, "p")
                                                            @Html.HiddenFor(x => x.InputByUser)
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Mobile Number", "Enter Mobile Number") <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.MobileNumber, new { @class = "form-control", id = "MobileNumber", maxlength = "10", required = "required", @type = "number", @autocomplete = "off" })
                                                            @Html.ValidationMessageFor(x => x.MobileNumber, "", new { @class = "help-block" }, "p")
                                                            <label id="mobileValidator" style="color:red;"></label>
                                                            @Html.HiddenFor(x => x.MobileNumber)
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-6 col-md-6" style="display:none">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Email Number", "Enter Email Id") <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.CustomerEmailId, new { @class = "form-control", id = "REgistrationCustomerEmailId" })
                                                            @Html.ValidationMessageFor(x => x.CustomerEmailId, "", new { @class = "help-block" }, "p")
                                                            @Html.HiddenFor(x => x.CustomerEmailId)
                                                        </div>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by city", "City") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.City, new SelectList(Model.CityList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchAreaOfCity(this.value,'#NewConArea')", @class = "form-control reset-control NewConCity", id = "NewConCity", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.City, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by area", "Area") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.Area, new SelectList(Model.AreaList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchSocietyByArea(this.value,'#NewConApartmentComplex')", @class = "form-control reset-control NewConArea", id = "NewConArea", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.Area, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by Society", "Society") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.ApartmentComplex, new SelectList(Model.ApartmentComplexList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchHouseBySociety(this.value,'#NewConHouseNumber')", @class = "form-control reset-control NewConApartmentComplex", id = "NewConApartmentComplex", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.ApartmentComplex, "", new { @class = "help-block" }, "p")
                                                            <p id="SocietyAddress1"></p>
                                                            <p id="SocietyAddress2"></p>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by HouseNumber", "HouseNumber") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.HouseNumber, new SelectList(Model.HouseNumberList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchHouseByNumber()", @class = "form-control reset-control NewConHouseNumber", id = "NewConHouseNumber", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.HouseNumber, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Mobile Number", "Enter Mobile Number") <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.MobileNumber, new { @class = "form-control", id = "MobileNumber", maxlength = "10", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.MobileNumber, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-12 col-md-12">
                                                        <div class="form-group">
                                                            <label for="" class="badge-pill text-alert"> <b>@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Mobile Number", "If you are not able to find your details from address, Kindly contact customer Care (079 4754 5252 / 079 2762 3264) to know your customer ID. ")</b></label>
                                                        </div>
                                                    </div>
                                                }
                                            }
                                            @if (Model.IsOTPSent)
                                            {
                                                <div class="">
                                                    <div class="col-lg-12 col-md-12">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Customer Id", "Enter Valid OTP") <span class="required">*</span></label>

                                                            @Html.TextBoxFor(x => x.OTPNumber, new { @class = "form-control", id = "OTPNumber", required = "required", @autocomplete = "off" })
