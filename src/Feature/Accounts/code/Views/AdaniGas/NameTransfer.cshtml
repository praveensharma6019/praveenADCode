                                                    </div>
                                                }
                                                if (Model.DocumentStatus0 == "Re-UploadDocument")
                                                {
                                                    <div class="col-lg-4 col-md-4">
                                                        <label class="form-label" for="customFile">@Model.DocumentStatus0</label>
                                                    </div>
                                                }
                                            }

                                        </div>
                                    }

                                </div>
                                @if (Model.PreStatus != "Approved" || Model.PreStatus != "Submitted")
                                {
                                    <div>
                                        <button type="button" class="btn btn-primary w-auto" name="ApproveNameTrnasfer" value="Approve" id="Approve_By_Admin">Approve</button>

                                        <button type="button" class="btn btn-primary w-auto" name="RejectNameTrnasfer" value="Reject" id="Reject_By_Admin">Reject</button>

                                        <button type="button" class="btn btn-primary w-auto" name="UpdateNameTrnasfer" value="Ask for additional Details / Documents" id="Additional_Details_By_Admin">Ask for additional Details / Documents</button>
                                    </div>
                                }


                            </div>
                        </div>
                    </section>
                }
            }
        </div>
    </div>
</section>

