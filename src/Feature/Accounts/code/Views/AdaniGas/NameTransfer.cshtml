@using Sitecore.Foundation.Dictionary.Extensions
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Configuration
@using CaptchaMvc;
@using CaptchaMvc.HtmlHelpers;
@using Sitecore.Feature.Accounts
@model Sitecore.Feature.Accounts.Models.ByCustomerId

@{
    //var CustomerTypeStyle = Model.CustomerValidationType == "1" ? "display:flex;" : "display:none";
    //var customerId = Model.CustomerID;

}
@if (Session["Message"] != null && !string.IsNullOrEmpty(Session["Message"].ToString()))
{
    <div class="form-group row">
        <input id="submit_message" type="hidden" value="@Session["Message"].ToString()" />
    </div>
}
<section class="name-transfer">
    <div class="container">
        <div>

            @using (Html.BeginRouteForm(MvcSettings.SitecoreRouteName, FormMethod.Post, new { id = "name-transfer-step1", @class = "name-transfer-step1", enctype = "multipart/form-data" }))
            {

                @Html.AddUniqueFormId()
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                @Html.HiddenFor(x => x.CustomerName)
                @Html.HiddenFor(x => x.RegisterdConsumerName)
                @Html.HiddenFor(x => x.OutstandingAmount)
                @Html.HiddenFor(x => x.PreStatus)
                if (Session["NameTransferLogin"] == null)
                {
                    <div>
                        <div class="panel">
                            <h3>Name Transfer</h3>
                            <h6 class="text-alert">
                                <span class="fas fa-exclamation-circle mr-2"></span>Name Transfer for Domestic Customers
                            </h6>

                            <div class="mt-3">
                                <div class="row" style="display: none;">
                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-group">
                                            <div class="mt-2">
                                                <label for="">Select Connection Type<span class="required">*</span></label>
                                                <br />
                                                <select name="connection_type" class="form-control btn dropdown-toggle bg-white text-dark border-secondary border" style="height: 50px;" aria-label="Default select example">
                                                    <option selected="" />Domestic
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @if (!Model.IsOTPValid)
                                {
                                    <div class="row mt-2">
                                        <div class="col-lg-12 col-md-12">
                                            <div class="form-group">
                                                <div class="form-check form-check-inline">
                                                    @Html.RadioButtonFor(model => model.ProceedWith, "1", new { id = "ProceedWithCustomerId", @class = "rbProceedWith mb-2 mr-2" })
