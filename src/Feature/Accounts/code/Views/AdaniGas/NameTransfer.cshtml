                                                </div>
                                                <div class="form-check form-check-inline">
                                                    @Html.RadioButtonFor(model => model.ProceedWith, "2", new { id = "ProceedWithSearch", @class = "rbProceedWith mb-2 mr-2" })
                                                    <label for="ProceedWithSearch">
                                                        Search Customer ID based on society & Area
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }

                                @if (Model.ProceedWith != null)
                                {
                                    if (Model.IsOTPValid)
                                    {
                                        @Html.HiddenFor(m => m.CustomerID)
                                        @Html.HiddenFor(m => m.MobileNumber)

                                        <div class="row mt-2">
                                            @if (!string.IsNullOrEmpty(Model.stepOne))
                                            {
                                                if (Model.PreStatus != "Submitted")
                                                {
                                                    <div class="col-lg-12">
                                                        <h2><b>Existing Owner Details</b></h2>
                                                    </div>

                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Customer ID </label>
                                                            @Html.TextBoxFor(x => x.CustomerID, new { @class = "form-control reset-control", id = "Customer ID", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Register Consumer Name </label>
                                                            @Html.TextBoxFor(x => x.CustomerName, new { @class = "form-control reset-control", id = "CustomerName", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">Connection Type </label>
                                                            @Html.TextBoxFor(x => x.ConnectionType, new { @class = "form-control reset-control", id = "ConnectionType", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">

                                                            <label for="">Meter Number </label>
                                                            @Html.TextBoxFor(x => x.MeterNumber, new { Value = @Model.MeterNumber, @class = "form-control reset-control", id = "MeterNumber", @autocomplete = "off", @readonly = "readonly" })
                                                            <small id="" class="form-text text-muted"></small>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
