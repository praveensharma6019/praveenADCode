                                                                        <div class="mt-3" id="housing-society">
                                                                            <label for="">SELECT SOCIETY TYPE<span class="required"></span></label>
                                                                            <div class="row mt-2">
                                                                                <div class="col-lg-12 col-md-12">
                                                                                    <div class="form-group">

                                                                                        @Html.RadioButtonFor(x => x.SocietyType, "Registered Housing Society", new { id = "Registered_Housing_Society", disabled = "disabled" }) Registered Housing Society
                                                                                        @Html.RadioButtonFor(x => x.SocietyType, "Unregistered Housing Society", new { id = "Unregistered_Housing_Society", disabled = "disabled" }) Unregistered Housing Society
                                                                                        @Html.ValidationMessageFor(x => x.SocietyType, "", new { @class = "help-block" }, "p")
                                                                                        @Html.HiddenFor(x => x.SocietyType)


                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    }
                                                                    @if (Model.ApplicationType == "Builder_Case")
