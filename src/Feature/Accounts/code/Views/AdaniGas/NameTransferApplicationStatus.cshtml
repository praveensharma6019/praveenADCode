                                                            @Html.TextBoxFor(x => x.InputByUser, new { @class = "form-control", id = "InputByUser", maxlength = "25", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.InputByUser, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="" style="display:flex;">
                                                                @Html.Sitecore().Dictionary("/Accounts/Quick Pay/Mobile Number", "Enter Mobile Number") <span class="required">*</span><h6 class="text-alert" style="font-size:10px; padding:5px; margin-bottom:0px; margin-left:12px;">
                                                                    <span class="fas fa-exclamation-circle mr-2"></span>Please enter your Mobile Number as per your name ransfer request Application
                                                                </h6>
                                                            </label>



                                                            @Html.TextBoxFor(x => x.MobileNumber, new { @class = "form-control", id = "MobileNumber", maxlength = "10", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.MobileNumber, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by city", "City") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.City, new SelectList(Model.CityList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchAreaOfCity(this.value,'#NewConArea')", @class = "form-control reset-control", id = "NewConCity", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.City, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by area", "Area") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.Area, new SelectList(Model.AreaList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchSocietyByArea(this.value,'#NewConApartmentComplex')", @class = "form-control reset-control", id = "NewConArea", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.Area, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by Society", "Society") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.ApartmentComplex, new SelectList(Model.ApartmentComplexList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @onchange = "searchHouseBySociety(this.value,'#NewConHouseNumber')", @class = "form-control reset-control", id = "NewConApartmentComplex", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.ApartmentComplex, "", new { @class = "help-block" }, "p")
                                                            <p id="SocietyAddress1"></p>
                                                            <p id="SocietyAddress2"></p>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Search by HouseNumber", "HouseNumber") <span class="required">*</span></label>
                                                            @Html.DropDownListFor(x => Model.HouseNumber, new SelectList(Model.HouseNumberList, "Value", "Text"), "--Please Select--", htmlAttributes: new { @class = "form-control reset-control", id = "NewConHouseNumber", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.HouseNumber, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6 col-md-6">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Mobile Number", "Enter Mobile Number") <span class="required">*</span></label>
                                                            @Html.TextBoxFor(x => x.MobileNumber, new { @class = "form-control", id = "MobileNumber", maxlength = "10", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.MobileNumber, "", new { @class = "help-block" }, "p")
                                                        </div>
                                                    </div>
                                                }
                                            }
                                            @if (Model.IsOTPSent)
                                            {
                                                <div class="">
                                                    <div class="col-lg-12 col-md-12">
                                                        <div class="form-group">
                                                            <label for="">@Html.Sitecore().Dictionary("/Accounts/Quick Pay/Customer Id", "Enter Valid OTP") <span class="required">*</span></label>

                                                            @Html.TextBoxFor(x => x.OTPNumber, new { @class = "form-control", id = "OTPNumber", required = "required" })
                                                            @Html.ValidationMessageFor(x => x.OTPNumber, "", new { @class = "help-block" }, "p")
                                                            <div>
                                                                <span style="font-size: 10px;">@Model.Message</span>
                                                            </div>
                                                            <small id="" class="form-text text-muted"></small>
                                                            <div id="Statuscountdown"></div>
                                                            <input type="button" name="StatusResendOtps" value="Resend" class="btn btn-primary" id="NameTransferStatusResendOtp" style="display:none" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-12 text-right ">
                                                    <input type="submit" name="ValidateOTP" value="Verify" class="btn btn-primary" @*style="background-color: black; color: white; border-color: black;" *@ />
                                                    <input type="submit" name="CancelRequest" formnovalidate="formnovalidate" value="Cancel" class="btn btn-secondary" />

                                                </div>
                                            }
                                            else
                                            {
                                                if (Model.ProceedWith == "1")
                                                {
                                                    <div class="col-lg-12 text-right ">
                                                        <input type="submit" name="validateById" value="Proceed" class="btn btn-primary" @*style="background-color: black; color: white; border-color: black;" *@ />
                                                        <input type="submit" name="CancelRequest" formnovalidate="formnovalidate" value="Cancel" class="btn btn-secondary" />
                                                    </div>
                                                }
                                                else
                                                {
                                                    <div class="col-lg-12 text-right ">
                                                        <input type="submit" name="validateBySearch" value="Proceed" class="btn btn-primary" @*style="background-color: black; color: white; border-color: black;" *@ />
                                                        <input type="submit" name="CancelRequest" formnovalidate="formnovalidate" value="Cancel" class="btn btn-secondary" />
                                                    </div>
                                                }
                                            }
                                        </div>
                                    }
                                }
                            </div>

                            <div class="col-lg-12 text-center check_app-status" style="display:none">
                                <p><b><span>New Application For Name Transfer</span></b></p>
                                <div><a href="/Name-Transfer">Login</a></div>
                            </div>

                        </div>

                    </div>
                }
            }
        </div>
    </div>
</section>

