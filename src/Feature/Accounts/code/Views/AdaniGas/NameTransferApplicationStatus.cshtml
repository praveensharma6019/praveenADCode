@using Sitecore.Foundation.Dictionary.Extensions
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Configuration
@using CaptchaMvc;
@using CaptchaMvc.HtmlHelpers;
@using Sitecore.Feature.Accounts
@model Sitecore.Feature.Accounts.Models.ByCustomerId

@{
    //var CustomerTypeStyle = Model.CustomerValidationType == "1" ? "display:flex;" : "display:none";
    //var customerId = Model.CustomerID;
}
@if (Session["Message"] != null && !string.IsNullOrEmpty(Session["Message"].ToString()))
{
    <div class="form-group row">
        <input id="submit_message" type="hidden" value="@Session["Message"].ToString()" />
    </div>
}
<section class="name-transfer">
    <div class="container">
        <div class="row">

            @using (Html.BeginRouteForm(MvcSettings.SitecoreRouteName, FormMethod.Post, new { id = "name-transfer-step1", @class = "col-12 nametransfer-applicationstatus", enctype = "multipart/form-data" }))
            {

                @Html.AddUniqueFormId()
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                @Html.HiddenFor(x => x.AddAdditionalPayment)
                @Html.HiddenFor(x => x.PreStatus)

                if (Session["NameTransferLogin"] == null)
                {
                    <div class="row">
                        <div class=" col-12 panel">
                            <h3 style="display: none;">Name Transfer</h3>
                            <h6 class="text-alert" style="display: none;">
                                <span class="fas fa-exclamation-circle mr-2"></span>This name transfer is
                                for domestic segment only
                            </h6>
                            <h2><b>Check Submitted Request</b></h2>
                            <div class="mt-3">
                                <div class="row" style="display: none;">
                                    <div class="col-lg-6 col-md-6">
                                        <div class="form-group">
                                            <div class="mt-2">
                                                <label for="">Select Connection Type<span class="required">*</span></label>
                                                <br />
                                                <select name="connection_type" class="form-control btn dropdown-toggle bg-white text-dark border-secondary border" style="height: 50px;" aria-label="Default select example">
                                                    <option selected="" />Domestic
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                @if (!Model.IsOTPValid)
                                {
                                    <div class="row mt-2">
                                        <div class="col-lg-12 col-md-12">
                                            <div class="form-group">
                                                <div class="form-check form-check-inline">
                                                    @Html.RadioButtonFor(model => model.ProceedWith, "1", new { id = "ProceedWithCustomerId", @class = "rbProceedWiths mb-2 mr-2" })
                                                    <label for="ProceedWithCustomerId">Proceed with Customer ID <b>or</b> Meter Number <b>or</b> Request No</label>
                                                </div>
                                                <div class="form-check form-check-inline" style="display: none;">
                                                    @Html.RadioButtonFor(model => model.ProceedWith, "2", new { id = "ProceedWithSearch", @class = "rbProceedWiths mb-2 mr-2" })
                                                    <label for="ProceedWithSearch">
                                                        Search customer ID based on society & Area Society
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }

                                @if (Model.ProceedWith != null)
                                {
                                    if (Model.IsOTPValid)
                                    {
                                        @Html.HiddenFor(m => m.CustomerID)
                                        @Html.HiddenFor(m => m.MobileNumber)

                                        if (Model.PreStatus == "Additional_Details/document_Required" || Model.PreStatus == "Additional_Details/document/Payment_Required")
                                        {
                                            @Html.HiddenFor(x => x.CustomerID)
                                            <section class="name-transfer submit_request">
                                                <div class="container">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            @Html.HiddenFor(x => x.id)
                                                            @Html.HiddenFor(x => x.PreStatus)
                                                            @Html.HiddenFor(x => x.MobileNumber)
                                                            @Html.HiddenFor(x => x.CustomerID)
                                                            @Html.HiddenFor(x => x.ShowisAdditionalDocumentRequired)
                                                            @Html.HiddenFor(x => x.AddAdditionalPayment)

                                                            <div class="tab-content" id="myTabContent">

                                                                <div class="col-lg-12 col-md-12">
                                                                    <div class="form-group">
                                                                        <label for="">Select Connection Type <span class="required">*</span></label>
                                                                        @Html.TextBoxFor(x => x.ConnectionType, new { @class = "form-control reset-control", id = "ConnectionType", @autocomplete = "off", required = "required", disabled = "disabled" })
                                                                        @Html.ValidationMessageFor(x => x.ConnectionType, "", new { @class = "help-block" }, "p")
                                                                        @Html.HiddenFor(x => x.ConnectionType)
                                                                        <small id="" class="form-text text-muted"></small>
                                                                    </div>
                                                                </div>

                                                                <div class="col-lg-12 col-md-12">

                                                                    <label for="">APPLICANT TYPE<span class="required"></span></label>

                                                                    <div class="row mt-2">
                                                                        <div class="col-lg-12 col-md-12">
                                                                            <div class="form-group">

                                                                                @Html.RadioButtonFor(x => x.ApplicationType, "Builder_Case", new { id = "Builder_Case", @onchange = "showinformationPopup(this.id)", disabled = "disabled" }) Builder Case
                                                                                @Html.RadioButtonFor(x => x.ApplicationType, "Property_ReSale", new { id = "Property_ReSale", @onchange = "showinformationPopup(this.id)", disabled = "disabled" })  Property Re-Sale
                                                                                @Html.RadioButtonFor(x => x.ApplicationType, "Demise", new { id = "Demise", @onchange = "showinformationPopup(this.id)", disabled = "disabled" }) Demise
                                                                                @Html.ValidationMessageFor(x => x.ApplicationType, "", new { @class = "help-block" }, "p")
                                                                                @Html.HiddenFor(x => x.ApplicationType)

                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    @if (Model.ApplicationType == "Property_ReSale" || Model.ApplicationType == "Demise")
