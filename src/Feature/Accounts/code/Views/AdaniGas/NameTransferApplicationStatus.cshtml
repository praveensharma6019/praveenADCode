                                                                        <div class="mt-3" id="GasSupply">
                                                                            <label for="">Do you want to start the gas supply?<span class="required">*</span></label>
                                                                            <div class="row mt-2">
                                                                                <div class="col-lg-12 col-md-12">
                                                                                    <div class="form-group">
                                                                                        @Html.RadioButtonFor(x => x.GasSupply, "Yes", new { id = "GasSupplyYes", disabled = "disabled" }) Yes
                                                                                        @Html.RadioButtonFor(x => x.GasSupply, "No", new { id = "GasSupplyNO", disabled = "disabled" }) No
                                                                                        @Html.HiddenFor(x => x.GasSupply)

                                                                                    </div>
                                                                                    <label class="text-danger" id="GasSupplyCheckError" style="display:none">This Field is required.</label>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    }


                                                                </div>

                                                                <div class="col-lg-12 col-md-12">
                                                                    <label>APPLICANT DETAILS<span class="required"></span></label>

                                                                    <div class="row mt-2">
                                                                        <div class="col-lg-4 col-md-4">
                                                                            <div class="form-group">
