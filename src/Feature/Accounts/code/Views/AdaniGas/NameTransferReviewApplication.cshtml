    var CreatedDate = Model.CreatedDate.ToString("MM/dd/yyyy");
    var role = UserSession.nameTransferAdminRegistationSessssion.Role;
    var BackUrl = string.Empty;
    if (Session["NameTransferSearchUrl"] != null && !string.IsNullOrEmpty(Session["NameTransferSearchUrl"].ToString()))
    {
        BackUrl = Session["NameTransferSearchUrl"].ToString();

    }
    else
    {
        BackUrl = "/NameTransferHome";
    }
}

@if (Session["Message"] != null && !string.IsNullOrEmpty(Session["Message"].ToString()))
{
