@using Sitecore.Feature.Identity
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Presentation
@model Sitecore.Mvc.Presentation.RenderingModel
@{
    /**/

    var datasource = Sitecore.Mvc.Presentation.RenderingContext.Current.Rendering.DataSource;
    var model = Sitecore.Context.Database.GetItem(datasource);
}

<div class="col-lg-3 pr-3 pl-1 txt-center compInfoFooter">
    <div class="row">
        
        <div class="col-lg-12 col-4 compLogosFooter">
            <a href="http://www.adanirealty.com" target="_blank"> @Html.Sitecore().ImageField(Templates.Identity.Fields.Logo, model, 0, 0, "img-fluid")</a>
            <img src="-/media/B51F791717A64C5C91F850D1DC5A75A7.ashx" class="img-fluid">
            <div class="d-md-block txt-gold mt-3">&copy; 2019 Adani Realty</div>
        </div>
    </div>
</div>

