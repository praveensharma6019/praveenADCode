@{ 

    var ContentPath = Sitecore.Context.Item.Paths.ContentPath.Replace("/Agrilogistics/", "");
    var path = ContentPath.Replace("/Agrilogistics/", "");
    string[] pathArr = path.Split('/');
    var navPath = "/";
}

<section class="breadcrumb-sub-layout bg-gray py-5">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <ul class="breadcrumb-sublayout-ul">
                    @for (int i = 0; i <= pathArr.Count() - 1; i++)
                    {
                        navPath = navPath + pathArr[i];


                        <li><a href="@navPath">@pathArr[i].Replace("-"," ")</a></li>
                        navPath = navPath + "/";
                    }
                </ul>
            </div>
        </div>
    </div>
</section>