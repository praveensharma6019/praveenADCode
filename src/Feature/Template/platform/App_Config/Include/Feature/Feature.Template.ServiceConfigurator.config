<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
		<pipelines>
			<initialize>
				<processor type="Sitecore.Feature.Template.Route.RegisterRoute, Sitecore.Feature.Template" patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" resolve="true"/>
			</initialize>
		</pipelines>
		<services>
			<configurator type="Sitecore.Feature.Template.ServiceConfigurator, Sitecore.Feature.Template" />
		</services>
	</sitecore>
</configuration>