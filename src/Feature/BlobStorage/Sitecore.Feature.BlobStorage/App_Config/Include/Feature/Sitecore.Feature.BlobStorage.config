<?xml version="1.0"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <initialize>
        <processor type="Sitecore.Feature.BlobStorage.Pipelines.RegisterWebApiRoutes, Sitecore.Feature.BlobStorage" patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeGlobalFilters, Sitecore.Mvc']"/>
      </initialize>
    </pipelines>
    <services>
      <configurator type="Sitecore.Feature.BlobStorage.ServiceConfigurator, Sitecore.Feature.BlobStorage" />
    </services>    
  </sitecore>
</configuration>


