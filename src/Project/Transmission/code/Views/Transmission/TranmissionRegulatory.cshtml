@model RenderingModel
@{
    
	
	
	
	 var ApplicableRegulationsdatasource = Sitecore.Mvc.Presentation.RenderingContext.Current.Rendering.Parameters["ApplicableRegulations"];
    var ApplicableRegulations = Sitecore.Context.Database.GetItem(ApplicableRegulationsdatasource);
	
	
	 var LicenseeOrdersdatasource = Sitecore.Mvc.Presentation.RenderingContext.Current.Rendering.Parameters["LicenseeOrders"];
