@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@{

    var datasource = Sitecore.Mvc.Presentation.RenderingContext.Current.Rendering.DataSource;
    var parent = Sitecore.Context.Database.GetItem(datasource);


}

<section class="">
    <div class="owl-carousel projects-carousel">
        @foreach (var item in parent.Children.ToList())
        {
            <div class="item">
                <div class="projects-carousel__item">
                    @Html.Sitecore().ImageField("MediaImage", item)
                    <h3>@Html.Sitecore().Field("MediaTitle", item)</h3>
                </div>
            </div>
        }
        </div>
</section>