    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>



    <!--Animate CSS-->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.0.0/animate.min.css" rel="stylesheet">

    <!--Open Sans Fonts-->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Lato&family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;1,300;1,400;1,500;1,600;1,700;1,800&display=swap" rel="stylesheet">

    <link href="/styles/DTPL/bootstrap.min.css" rel="stylesheet" />
    <link href="/styles/DTPL/fontawesome-all.css" rel="stylesheet" />
    <link href="/styles/DTPL/owl.carousel.css" rel="stylesheet" />
    <link href="/styles/DTPL/owl.theme.default.css" rel="stylesheet" />
    <link href="/styles/DTPL/adani-custom.css" rel="stylesheet" />
    <link href="/styles/DTPL/adani-custom-responsive.css" rel="stylesheet" />
    <link href="~/styles/DTPL/Dashboard.css" rel="stylesheet" />
    <link href="~/styles/DTPL/Login.css" rel="stylesheet" />

    <!-- Latest compiled and minified JavaScript -->
    @RenderAssetsService.Current.RenderScript(ScriptLocation.Head)

    @RenderAssetsService.Current.RenderStyles()

    @Html.Sitecore().VisitorIdentification()
    <script src="/scripts/DTPL/jquery.min.js"></script>
    <script src="/scripts/DTPL/popper.min.js"></script>
    <script src="/scripts/DTPL/bootstrap.min.js"></script>
    <script src="/scripts/DTPL/owl.carousel.min.js"></script>
    <script src="/scripts/DTPL/adani-custom.js"></script>

    <script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit" async defer></script>
    <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>



</head>
<body class=" @(Sitecore.Context.PageMode.IsNormal ? "" : (Sitecore.Context.PageMode.IsExperienceEditor ? "pagemode-edit" : "pagemode-preview"))">
