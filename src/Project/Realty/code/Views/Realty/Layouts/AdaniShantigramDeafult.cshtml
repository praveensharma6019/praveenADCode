        function zoomin() {
            var myImg = $("#map");
            var currWidth = myImg.outerWidth();
            if (currWidth == 2500) return false;
            else {
                myImg.css('width', currWidth + 100) + "px";
            }
        }

        function zoomout() {
            var myImg = $("#map")
            var currWidth = myImg.outerWidth();
            if (currWidth == 100) return false;
            else {
                myImg.css('width', currWidth - 100) + "px";
            }
        }
