    <style>
	.property-booking{}
	.property-booking .nav{}
	.property-booking .nav .nav-item{margin-right: 15px;}
	.property-booking .nav .nav-item .nav-link{font-size: 17px; border: 0px; padding: 10px 20px;background: #ffffff; /* Old browsers */ background: -moz-linear-gradient(top,  #ffffff 0%, #ededed 100%); /* FF3.6-15 */ background: -webkit-linear-gradient(top,  #ffffff 0%,#ededed 100%); /* Chrome10-25,Safari5.1-6 */ background: linear-gradient(to bottom,  #ffffff 0%,#ededed 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */ filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ededed',GradientType=0 ); /* IE6-9 */border: 1px solid; border-color: #e6e6e6;color: #2b7aab;}
	.property-booking .nav .nav-item .nav-link i{font-size: 12px;  padding: 1px 5px; border-radius: 100px;  font-style: normal; width: 21px; display: inline-block; text-align: center; margin-right: 7px;position: relative; top: -1px;background: #2c76ad; color: #fff;}
	.property-booking .nav .nav-item .nav-link.active i{background: #fff;color: #2f70b1;}
	.property-booking .nav .nav-item .nav-link.active{
		font-weight: 100;
		background: #2f70b1; /* Old browsers */
		background: -moz-linear-gradient(top,  #2f70b1 0%, #1b9d95 100%); /* FF3.6-15 */
		background: -webkit-linear-gradient(top,  #2f70b1 0%,#1b9d95 100%); /* Chrome10-25,Safari5.1-6 */
		background: linear-gradient(to bottom,  #2f70b1 0%,#1b9d95 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2f70b1', endColorstr='#1b9d95',GradientType=0 ); /* IE6-9 */
		color:#fff !important;border-color: #287fa7;}
	.property-booking .tab-content{background: #fff; box-shadow: 0px 0px 6px -2px #a7a7a7; z-index: 9; position: relative;}
	.property-booking .tab-content select{border: 0px; border-bottom: 2px solid #2d76ae;background: transparent;box-shadow: 0px 0px 0px transparent !important;font-size: 1rem;}
	.property-booking .tab-content h3{color: #2b7aab;}
	.property-booking .tab-content .bg-grey{background: #f5f5f5;}
	.property-booking .tab-content .location-filter{}
	.property-booking .tab-content .location-filter li{position:relative;padding-left: 50px;}
	.property-booking .tab-content .location-filter li i{position:absolute;left:0px;width: 30px; top: 16px;}
	.property-booking .tab-content .configuration{}
	.property-booking .tab-content .configuration h5{font-size: 1rem;color: #5c5c5c;}
	.property-booking .tab-content .configuration p{font-size: 1.25rem;font-weight: 700;}
	.property-booking .tab-content .configuration img{max-width: 35px;}
	.property-booking .tab-content label{font-size: 1.25rem; font-weight: 700;}
	.property-booking .tab-content .property-details p i{color: #757575;font-size: 1.25rem;}
	.property-booking .tab-content .property-details h5{color: #757575;font-size: 1rem;}
	.property-booking .tab-content .property-details p{font-size: 1.25rem; font-weight: 800; color: #757575;}
	.btn-bordered{color: #16498a; border:2px solid #16498a;padding: 10px 20px; font-size: 20px; position: relative;transition:ease all 0.5s;}
	.btn-bordered:hover{background:#16498a;color:#fff;transition:ease all 0.5s;}
	.property-booking .tab-content .personal-details{}
	.property-booking .tab-content .personal-details label{color: #000; font-weight: 100; font-size: 1.25rem; margin-bottom: 0.5rem;}
	.property-booking .tab-content .personal-details select{color: #9a9a9a;}
	.property-booking .tab-content .personal-details input{border: 0px; background: #f8f8f8; padding: 10px 10px;}
	.property-booking .tab-content .personal-details textarea{border: 0px; background: #f8f8f8; padding: 10px 10px;}
	.property-booking .tab-content .personal-details input[type="checkbox"]{position: relative; top: 30px;}
	.property-booking .tab-content .personal-details label.form-check-label{color: #000;}
	.property-booking .tab-content .text-center p i{font-size: 0.6rem; position: relative; top: -2px;}
	.property-booking .tab-content .form-agree{}
	.property-booking .tab-content .form-agree label{font-size: 1rem; font-weight: 100; padding-left: 5px;}
	.property-booking .tab-content .col-lg-12 p.mb-2{font-size: 13px;}
	a.btn:not([href]):not([tabindex]){color:#fff;}
	@@media (min-width: 992px){
		.border-right-lg{border-right: 1px solid #ccc;}
	}
	@@media (max-width: 991px){
		.border-right-lg{border-bottom: 1px solid #ccc;}
	}
	@@media (min-width: 768px){
		.property-booking .tab-content .collapse{display:block;}
	}
	@@media (max-width: 767px){
		.tab-pane.active .collapse{display:block;}
		.tab-pane .collapse{display:none;}
		.property-booking .tab-content .tab-pane .btn-bordered, .property-booking .tab-content .tab-pane .btn-primary{font-size: 14px; padding: 5px 10px;}
		.property-booking .tab-content .tab-pane button{width: 100%; text-align: left; background: #ffffff; background: -moz-linear-gradient(top, #ffffff 0%, #ededed 100%); background: -webkit-linear-gradient(top, #ffffff 0%,#ededed 100%); background: linear-gradient(to bottom, #ffffff 0%,#ededed 100%); filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ededed',GradientType=0 ); border: 1px solid; border-color: #e6e6e6; color: #2b7aab; font-weight: 600;padding: 5px 10px;position:relative;cursor: pointer;}
		.property-booking .tab-content .tab-pane button i{font-size: 12px; padding: 1px 5px; border-radius: 100px; font-style: normal; width: 21px; display: inline-block; text-align: center; margin-right: 7px; position: relative; top: -1px; background: #2c76ad; color: #fff;}
		.property-booking .tab-content .tab-pane button:after{content:"\f105";font-family: 'Font Awesome 5 Free'; font-weight: 900;position: absolute; right: 10px;transform: rotate(90deg); transition: ease all 0.5s;}
		.property-booking .tab-content .tab-pane button.collapsed:after{content:"\f105";font-family: 'Font Awesome 5 Free'; font-weight: 900;position: absolute; right: 10px;transform: rotate(0deg); transition: ease all 0.5s;}
		.property-booking .tab-content .tab-pane button{}
		.property-booking .tab-content .tab-pane{display: block;opacity: 1;margin-bottom: 1rem;}
		.property-booking .tab-content{background: transparent;box-shadow: 0px 0px 6px -2px transparent;}
		.personal-details{background:#fff;}
		.property-booking .tab-content .personal-details label.form-check-label{position: relative; top: -30px;}
	}
    </style>


