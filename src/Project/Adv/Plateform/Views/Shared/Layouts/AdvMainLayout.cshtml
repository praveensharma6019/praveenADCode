@using Adani.BAU.Adv.Project.Helper;
@using Sitecore.Mvc;
@using Adani.BAU.Adv.Project.Helper;
@using Adani.BAU.Adv.Project;
@using System.Web.Mvc;
@inherits System.Web.Mvc.WebViewPage
@{

    string PageId = string.Empty;
    if (Sitecore.Context.Item != null)
    {
        PageId = Convert.ToString(Sitecore.Context.Item.ID);
    }
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width" />


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.css" />
    <link href="~/assets/css/aos.css" rel="stylesheet" />
    <!--<link href="~/assets/css/advstyle.css" rel="stylesheet" />-->
    <link href="~/-/media/advassets/advstyle.css" rel="stylesheet" />
    @Html.Raw(@ContentHelper.GTMHeader(Constant.strGTMScriptHeadBodyTag))
</head>
<body>
    @Html.Raw(@ContentHelper.GTMBody(Constant.strGTMScriptHeadBodyTag))
    <input type="hidden" value="@PageId" />
    <div class="header">
        @Html.Raw(@ContentHelper.Navigation(Constant.strHeader))

    </div>
    <div>
        @Html.Partial("/Views/Shared/Layouts/cookiePopup.cshtml");
        @Html.Raw(@Html.Sitecore().Field("Text"))
    </div>
    @*fooder hide for portfolio detail page and team details page*@
    @if (PageId != "{19FCFE15-C860-4A81-A1CB-1A48AE0117A0}" && PageId != "{9610C275-8161-44E3-B5EA-BE2C7E9C70D9}")
    {
        <div class="footer">

            @Html.Raw(@ContentHelper.Navigation(Constant.strFooter))


        </div>
    }
    <!--<script type="module" src="./src/ADV/adani-disruptive-ventures/index.js"></script>-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/slick.min.js"></script>
    <script defer src="~/assets/js/aos.js"></script>
    <!--<script defer src="~/assets/js/advmain.js"></script>-->
    <script defer src="~/-/media/advassets/advmain.js"></script>





</body>
</html>
