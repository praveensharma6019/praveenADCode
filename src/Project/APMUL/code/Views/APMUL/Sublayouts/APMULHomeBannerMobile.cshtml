@using Sitecore.Mvc.Presentation
@using Sitecore.Foundation.SitecoreExtensions.Extensions

@{
    var datasource = Sitecore.Mvc.Presentation.RenderingContext.Current.Rendering.DataSource;
    var elements = Sitecore.Context.Database.GetItem(datasource);
   var NavigateUrl="";
}

	
	<div class="owl-carousel owl-theme homeslider 123" data-click-state="0">
        @foreach (var item in elements.Children.ToList())
        {
		        Sitecore.Data.Fields.LinkField linkField = item.Fields["MediaVideoLink"];
				NavigateUrl = linkField.Url;
<div class="item @NavigateUrl" data-videosrc="@NavigateUrl">
    @Html.Sitecore().ImageField("MediaThumbnail", item, 0, 0, "image-fluid")
</div>
           
        }
		  </div>
	
