<script>
  
$(document).ready(function() {
	if (window.location.href.indexOf("commercial") >= 0 || window.location.href.indexOf("industrial") >= 0) {
		$('#firstNameFPLabel').hide();
		$("#firstName").hide();
		$('#firstNameFPText').hide();
		$('#lastNameFPLabel').html("Organization Name");
		$('#lastNameFPText').html("Organization name should be as per your gas invoice with first character as capital");
	}
	else{
		$('#firstNameFPLabel').show();
		$("#firstName").show();
		$('#firstNameFPText').show();
		$('#lastNameFPLabel').html("Last Name");
		$('#lastNameFPText').html("Last name should be as per your gas invoice with first character as capital");
	}
});

function check(e,value)
{
    //Check Charater
    var unicode=e.charCode? e.charCode : e.keyCode;
    if (value.indexOf(".") != -1)if( unicode == 46 )return false;
    if (unicode!=8)if((unicode<48||unicode>57)&&unicode!=46)return false;
}
function checkLength()
