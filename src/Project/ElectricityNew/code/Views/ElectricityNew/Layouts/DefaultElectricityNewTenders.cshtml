@using Sitecore.Foundation.Assets
@using Sitecore.Foundation.Assets.Models
@using Sitecore.Foundation.Assets.Services
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Analytics.Extensions
@using Sitecore.ExperienceForms.Mvc.Html
@using Sitecore.Foundation.Dictionary.Extensions
@using Sitecore.Mvc
@inherits WebViewPage
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="@Sitecore.Context.Language.CultureInfo.TwoLetterISOLanguageName">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Adani Electricity</title>
    <link rel="stylesheet" href="/electricity_assets/css/materialize.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css" />
    <link rel="stylesheet" href="/electricity_assets/styles/main.css" />
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    @*<link href="/styles/ElectricityNew/style.css" rel="stylesheet">
    <script src="/scripts/ElectricityNew/jquery-1.10.2.min.js"></script>*@

    
    <!-- Latest compiled and minified JavaScript -->
    @*@RenderAssetsService.Current.RenderScript(ScriptLocation.Head)

    @RenderAssetsService.Current.RenderStyles()

    @Html.Sitecore().VisitorIdentification()*@

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

    @if (HttpContext.Current.Request.Url.AbsoluteUri.Contains("DownloadPayBill"))
    {
        <script type="module" src="/Scripts/Accounts/DownloadPayBillRevamp.js"></script>
    }
    @if (HttpContext.Current.Request.Url.AbsoluteUri.Contains("PaymentHistory"))
    {
        <script type="module" src="/Scripts/Accounts/PaymentHistoryRevamp.js"></script>
    }
    @if (HttpContext.Current.Request.Url.AbsoluteUri.Contains("ConsumptionHistory"))
    {
        <script type="module" src="/Scripts/Accounts/ConsumptionHistoryRevamp.js"></script>

    }
    @if (HttpContext.Current.Request.Url.AbsoluteUri.Contains("EnergyCalculator"))
    {
        <script type="module" src="/scripts/electricitynew/energycalculator.js"></script>
    }

</head>
<body>
    <header class="main-header not-floating">
        <ul class="flx-list-wrapper top-bar hide-on-med-and-down">

            @Html.Sitecore().Placeholder("header-top-bar")
        </ul>
        <ul class="header-nav flx-list-wrapper">
            @Html.Sitecore().Placeholder("header-primary-menus")
        </ul>
    </header>
    <div id="slide-out" class="sidenav hamburger">
        <ul class="collapsible0 expandable0 hamburger_section hamburger-header single-item">
            <li>
                @Html.Sitecore().Placeholder("hamburger-navigation-menu")
            </li>
        </ul>

        @Html.Sitecore().Placeholder("hamburger-navigation-submenu")

    </div>
    <div class="container">
        @Html.Sitecore().Placeholder("page-layout")
    </div>
    @*@RenderAssetsService.Current.RenderScript(ScriptLocation.Body)
        @Html.RenderFormScripts()*@



    @Html.Sitecore().Placeholder("ElectricityNewFooter")

    @*@RenderAssetsService.Current.RenderScript(ScriptLocation.Body)
    @Html.RenderFormScripts()
    <script src="/scripts/Electricity/hygieneheader.js"></script>*@

    <!-- Compiled and minified JavaScript -->
    <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    <script type="module" src="/electricity_assets/assets/js/main.js"></script>

   
</body>

</html>
