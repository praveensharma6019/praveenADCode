@using Sitecore.Foundation.Assets
@using Sitecore.Foundation.Assets.Models
@using Sitecore.Foundation.Assets.Services
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Analytics.Extensions
@using Sitecore.ExperienceForms.Mvc.Html
@using Sitecore.Foundation.Dictionary.Extensions
@using Sitecore.Mvc
@using Sitecore.Feature.Accounts.SessionHelper
@inherits WebViewPage
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="@Sitecore.Context.Language.CultureInfo.TwoLetterISOLanguageName">
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    @Html.Sitecore().Placeholder("head")
    <link rel="stylesheet" href="/electricity_assets/css/materialize.css" />
    <link href="/styles/ElectricityNew/aeml-revamp-owl-carousel.css" rel="stylesheet" />
    <link rel="stylesheet" href="/electricity_assets/styles/main.css" />
    <script src="/scripts/ElectricityNew/materialize-aeml-revamp.js"></script>
    <script type="text/javascript" src="/scripts/ElectricityNew/jquery-aeml-revamp.js"></script>
    <script src="/scripts/ElectricityNew/CommanSitecoreSettings.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
</head>
<body>
    @Html.Sitecore().Placeholder("gtm-body")

    <div class="custom-loader loader-wrap page-loader" style="display:none;">
        <div class="custom-loader-box-wrapper">
            <span class="custom-loader-dots-box">
                <span class="custom-loader-dot custom-loader-dot-1"></span>
                <span class="custom-loader-dot custom-loader-dot-2"></span>
                <span class="custom-loader-dot custom-loader-dot-3"></span>
            </span>
            <span class="custom-loader-text">Give us a moment. We're almost there.</span>
        </div>
    </div>

    <section class="ev-charger-main">
        <header class="main-header ev-header-new" id="main">
            @Html.Sitecore().Placeholder("ev-hamburger")

            <div class="container">
                <div class="headerev">
                    <div class="hamburger" id="hamburgerev">
                        <a class="btn waves-effect waves-light hover-icon">
                            <i class="i-hamburger i-18"></i>
                        </a>
                    </div>
                    <div class="brand-logo">
                        <a href="/sharecharge">
                            <img src="/electricity_assets/images/logoev.png" />
                        </a>
                    </div>
                    <div class="social-icon">
                        @if (Sitecore.Context.Item.ID.ToString() == "{70921132-13B0-4201-9D9F-4E8F0DA292CD}")
                        {
                            <a href="#evchargingrequest"
                               data-gaevent="ev_connection_start"
                               data-gaeventCategory="AEML"
                               data-gaeventAction="ev_connection_start"
                               data-gaeventLabel="Sharecharge"
                               class="btn sidenav-trigger waves-effect waves-light ">I am interested</a>
                        }
                        else
                        {
                            <a href="/sharecharge?evaction=evchargingrequest"
                               data-gaevent="ev_connection_start"
                               data-gaeventCategory="AEML"
                               data-gaeventAction="ev_connection_start"
                               data-gaeventLabel="Sharecharge"
                               class="btn sidenav-trigger waves-effect waves-light">I am interested</a>
                        }
                        @*<ul class="social_link_list flx-list">
                                <li>
                                    <a target="_blank" href="https://www.facebook.com/AdaniElectricityMumbai/" class="waves-effect icon-holder small-size"><i class="i-facebook i-22"></i></a>
                                </li>
                                <li>
                                    <a target="_blank" href="https://www.instagram.com/adanielectricitymumbai" class="waves-effect icon-holder small-size"><i class="i-instagram i-22"></i></a>
                                </li>
                                <li>
                                    <a target="_blank" href="https://twitter.com/Adani_Elec_Mum" class="waves-effect icon-holder small-size"><i class="i-twitter i-22"></i></a>
                                </li>
                            </ul>*@
                    </div>
                </div>
            </div>
        </header>
        <div class="main-header ev-header-new header-shimmer">
           <div class="container">
                <div class="flx-list-wrapper">
                    <div class="flx-list">
                        <div class="icon animate"></div>
                        <div class="nav_item animate logo"></div>
                    </div>
                    <div class="flx-list">
                        <div class="nav_item animate button"></div>
                    </div>
                </div>
            </div>
        </div>
        @Html.Sitecore().Placeholder("page-layout")
    </section>

    <div class="ev-new-footer">
        <div class="container">
            <div class="content">
                <div class="copy"> &copy; 2022 Adani Electricity</div>
                <div class="social-icon">
                    <ul class="social_link_list flx-list">
                        <li>
                            <a target="_blank" href="https://www.facebook.com/AdaniElectricityMumbai/" class="waves-effect icon-holder small-size"><i class="i-facebook i-22"></i></a>
                        </li>
                        <li>
                            <a target="_blank" href="https://www.instagram.com/adanielectricitymumbai" class="waves-effect icon-holder small-size"><i class="i-instagram i-22"></i></a>
                        </li>
                        <li>
                            <a target="_blank" href="https://twitter.com/Adani_Elec_Mum" class="waves-effect icon-holder small-size"><i class="i-twitter i-22"></i></a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <button id="back-to-top">
        <img src="/electricity_assets/images/back-to-top_00.svg" alt="Back To Top" />
    </button>
    <script type="text/javascript" src="/scripts/ElectricityNew/owl-carousel-aeml-revamp.js"></script>
    <script type="text/javascript" src="/scripts/ElectricityNew/electricityrevampcustom.js"></script>
    <script src="/scripts/ElectricityNew/aeml-main.js"></script>
    <script src="/scripts/ElectricityNew/aes.js"></script>
    @if (Sitecore.Context.Item.ID.ToString() == "{70921132-13B0-4201-9D9F-4E8F0DA292CD}")
    {
        <script type="text/javascript">
            $(document).ready(function () {
                //$('body').attr("data-page", "home");

                $('header.header-corporate').removeClass('not-floating').addClass('floating');
            });
        </script>
    }

    <script type="text/javascript">
        $('a').click(function () {
            if ($(this).attr('data-gaevent') != undefined) {
                dataLayer.push({
                    'event': $(this).attr('data-gaevent'),
                    'eventCategory': $(this).attr('data-gaeventCategory'),
                    'eventAction': $(this).attr('data-gaeventAction'),
                    'eventLabel': $(this).attr('data-gaeventLabel'),
                    'business_user_id': $('#BusinessUserId').val(),
                    'login_status': $('#login_status').val(),
                    'ca_number': $('#GACANumber').val(),
                    'page_type': $('head title').text(),
                    'click_text': $(this).attr('clicked_text'),
                });
            }
        })
        $('button').click(function () {
            if ($(this).attr('data-gaevent') != undefined) {
                dataLayer.push({
                    'event': $(this).attr('data-gaevent'),
                    'eventCategory': $(this).attr('data-gaeventCategory'),
                    'eventAction': $(this).attr('data-gaeventAction'),
                    'eventLabel': $(this).attr('data-gaeventLabel'),
                    'business_user_id': $('#BusinessUserId').val(),
                    'login_status': $('#login_status').val(),
                    'ca_number': $('#GACANumber').val(),
                    'page_type': $('head title').text()
                });
            }
        })
    </script>

</body>
</html>