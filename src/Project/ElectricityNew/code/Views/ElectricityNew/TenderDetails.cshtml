@using Sitecore.Foundation.Dictionary.Extensions
@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Mvc.Configuration
@model Sitecore.ElectricityNew.Website.Model.Tender


<section class="corporate-main-banner section-container">
    <div class="inner">
        <figure>
            <picture>
                <source media="(min-width:992px)" srcset="-/media/8693F40A7CC2485F82B83EC9C05E7C3D.ashx" alt="Tender Details">
                <img alt="Open Tenders" class="banner-image" src="-/media/20AC3F0692BF465DBE5679E054A99947.ashx" />
            </picture>
        </figure>
        <div class="content">
            <div class="container">
                <div class="inner">
                    <h1>Tenders Details</h1>
                </div>
            </div>
        </div>
    </div>
</section>
<div class="container tenders-detail">
    <div class="row">
        <div class="col s12">
            @if (!string.IsNullOrEmpty(ViewBag.Message))
            {

                <input id="message" type="hidden" value="@ViewBag.Message" />


                if (ViewBag.MessageBoxTitle == "Thank You")
                {
                    <div id="tenderDetailstabone" class="modal modal-alert modal-success">
                        <header class="modal-header">
                            <h4><i class="i-tick"></i>@ViewBag.MessageBoxTitle</h4>
                            <span class="icon-holder waves-effect waves-light modal-close close"><i class="i-cross hide-notification"></i></span>
                        </header>

                        <div class="modal-content">
                            @if (!string.IsNullOrEmpty(ViewBag.Message))
                            {
                                <p>@ViewBag.Message</p>

                            }
                        </div>
                    </div>
                }
                else
                {
                    <div id="tenderDetailstabone" class="modal modal-alert modal-toast custom-toast-error">
                        <header class="modal-header">
                            <h4>
                                <a class="toast-close">
                                    <i class="i-cross"></i>
                                </a>
                                Error
                            </h4>
                            <a class="modal-close" id="closeIcon">
                                <i class="i-cross"> </i>
                            </a>
                        </header>
                        <div class="modal-content">
                            <p id="ToastAlertContent">@ViewBag.Message</p>
                        </div>
                    </div>
                }
            }

            <div class="tender-header">
                <div class="tender-main-header">
                    <div>
                        <h2>Tender Details</h2>
                        <p>Proceed here for tender submission</p>
                    </div>

                    <div class="header-links hide-on-small-only">
                        @*<a href="/tenderlogin" class="link-btn">@Html.Sitecore().Dictionary("Electricity/Tender/Logout", "Logout")</a>*@

                        <a href="javascript:void(0)" class="link-btn" onclick="document.getElementById('formLogout').submit();">
                            @Html.Sitecore().Dictionary("Electricity/Tender/Logout", "Logout")
                        </a>
                        <a href="/TenderChangePassword" class="link-btn"> @Html.Sitecore().Dictionary("Electricity/Tender/Change Password Label", "Change Password")</a>
                    </div>
                    <form action="@Url.Action("Logout","ElectricityNew")" id="formLogout" method="post" style="display:none">
                        @Html.Sitecore().Dictionary("Electricity/Tender/Logout", "Logout")
                    </form>

                </div>
            </div>
            <section class="section-container">
                <div class="section-header hide-on-small-only">
                    <h4>Existing Tender Details</h4>
                </div>
                @if (Model != null)
                {
                    <div class="card outlined brand-gradient card-section">
                        <div class="header-text">
                            <ul>
                                <li>
                                    <label>Name</label>
                                    <p>@Model.UserName</p>
                                </li>
                                <li>
                                    <label>Company Name</label>
                                    @Model.CompanyName
                                </li>
                                <li>
                                    <label>Emain ID </label>
                                    @Model.Email
                                </li>
                                <li>
                                    <label>Mobile Number</label>
                                    @Model.Mobile
                                </li>
                                <li>
                                    <label>Tender ID</label>
                                    @Model.NITNo

                                </li>
                                <li>
                                    <label>Tender Name</label>
                                    @Model.TenderName
                                </li>
                                <li>
                                    <label>Document Upload Status:</label>
                                    @Model.DocumentStatus
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="download">

                        @if (Model.tenderdocument != null && Model.tenderdocument.Count > 0)
                        {
                            <ul>
                                @foreach (var item in Model.tenderdocument)
                                {
                                    <li>
                                        Download:  <a href="@item.DocumentPath" target="_blank" download="" class="txt-orange">@item.FileName</a>
                                    </li>

                                }
                            </ul>
                        }
                    </div>

                }


                <div class="step-progress-bar stepper-wrapper">
                    <h5>Submit your request in 3 simple steps</h5>
                    <div class="row">
                        <div class="col s12 m8">
                            <div class="section-wrapper">
                                <ul class="stepper-header" id="stepper-holder" data-stepper-content-wrapper="stepperContent" data-stepper-content-holder="stepper-content-body">
                                    <li class="complete stepper-action active-step" data-content-target="content1">
                                        1. Tender Fee and EMD
                                        <span></span>
                                    </li>
                                    <li class="stepper-action" data-content-target="content2">
                                        2. Upload Envelope
                                        <span></span>
                                    </li>
                                    <li class="stepper-action" data-content-target="content3">
                                        3. View Envelope
                                        <span></span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

