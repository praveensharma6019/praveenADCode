<?xml version="1.0"?>

<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<sites>
			<site name="westerntransemission" patch:after="site[@name='modules_website']"
                  targetHostName="local.westerntransmission.com"
                  database="web"
                  virtualFolder="/"
                  physicalFolder="/"
                  rootPath="/sitecore/content/Westerntrans"
                  startItem="/Home"
                  dictionaryPath="/sitecore/content/AdaniTransemission/Global/Western Transmission Dictionary"
                  dictionaryAutoCreate="false"
                  placeholderSettingsRoot="/sitecore/layout/Placeholder Settings/Project/Adani/AdaniTransmission"
                  domain="extranet"
                  allowDebug="true"
                  cacheHtml="true"
                  htmlCacheSize="50MB"
                  registryCacheSize="0"
                  viewStateCacheSize="0"
                  xslCacheSize="25MB"
                  filteredItemsCacheSize="10MB"
                  enablePreview="true"
                  enableWebEdit="true"
                  enableDebugger="true"
                  disableClientData="false"
                  cacheRenderingParameters="true"
                  renderingParametersCacheSize="10MB"
                  dictionaryDomain="{EE80EF08-135C-44BD-AB93-2ACF3B3DE13B}"
                  enableItemLanguageFallback="true" />
		</sites>
		<cacheSizes>
			<sites>
				<westerntransemission>
					<html>50MB</html>
					<registry>0</registry>
					<viewState>0</viewState>
					<xsl>25MB</xsl>
				</westerntransemission>
			</sites>
		</cacheSizes>
		<settings>
			<setting name="Preview.DefaultSite">
				<patch:attribute name="value" value="westerntransemission" />
			</setting>
			<setting name="ServerTimeZone" value="UTC" />
			<globalization requestEncoding="utf-8" responseEncoding="utf-8" />
		</settings>
		<mediaLibrary>
			<mediaTypes>
				<mediaType name="PDF file" extensions="pdf">
					<forceDownload>false</forceDownload>
				</mediaType>
			</mediaTypes>
		</mediaLibrary>
	</sitecore>
</configuration>