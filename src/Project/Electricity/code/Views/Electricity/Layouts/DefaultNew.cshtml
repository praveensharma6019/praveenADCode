</head>
<body class=" @(Sitecore.Context.PageMode.IsNormal ? "" : (Sitecore.Context.PageMode.IsExperienceEditor ? "pagemode-edit" : "pagemode-preview"))" onunload="bodyUnload();" Onclick="clicked=true;">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-5TZJNCL"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
	@Html.Sitecore().Placeholder("header-new")
    
    <main role="main">
        @Html.Sitecore().Placeholder("breadcrumb")
        @Html.Sitecore().Placeholder("page-layout")
    </main>

    <footer class="footerSec">
        @Html.Sitecore().Placeholder("footer")
    </footer>

    @Html.Sitecore().Placeholder("page-sidebar")

    @RenderAssetsService.Current.RenderScript(ScriptLocation.Body)
    @Html.RenderFormScripts()
