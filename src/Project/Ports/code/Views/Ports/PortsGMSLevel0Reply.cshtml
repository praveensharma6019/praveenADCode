                            </div>


                            <div class="col-lg-4">
                                <label for="">Point Man</label>
                                <div class="form-group">
                                    <input type="text" class="form-control" name="PointMan" value="@Model.PointMan" placeholder="" readonly>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="form-group">
                                    <label for="">Level-2 Comment(@Model.Level2UserName)</label>
                                    @*<input type="text" class="form-control" name="Level1Comment" value="@Model.Level2Comment" placeholder="" readonly>*@
                                    <textarea class="" name="" rows="3" cols="75" readonly style="background-color: #deeaf5;">@Model.Level2Comment</textarea>
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label for="">Level-3 Comment(@Model.Level3UserName)</label>
                                    @*<input type="text" class="form-control" name="Level1Comment" value="@Model.Level3Comment" placeholder="" readonly>*@
                                    <textarea class="" name="" rows="3" cols="117" readonly style="background-color: #deeaf5;">@Model.Level3Comment</textarea>
                                </div>
                            </div>
                            @*</div>*@
                            @foreach (var stakeholder in Model.StakeholderFinalComment)
                            {
                                <div class="col-lg-12">
                                    <label for="">Level-0 Final Comment(@stakeholder.UserName) [@stakeholder.Created_Date]</label>
                                    <div class="form-group">
                                        <textarea class="" name="Level0Comment" rows="3" cols="117" minlength="50" readonly style="background-color: #deeaf5;">@stakeholder.FinalComment</textarea>
                                    </div>
                                </div>
                            }
                            <div class="col-lg-12">
                                <div class="form-group">
                                    <label for="">Level-0 Final Comment(@Model.Level0UserName)</label>
                                    @*<textarea class="form-control" id="" rows="3"></textarea>*@
                                    <textarea class="" name="FinalComment" minlength="50" id="FinalComment" rows="3" cols="117"></textarea>
                                </div>
                            </div>
                            <div class="col-lg-12 mt-3 text-right">
                                @*@*<button type="submit" class="btn btn-gradient">Submit to Stackholder</button>*@
                                <input id="submitToStackholder" class="submitToStackholder btn btn-gradient" type="button" value="Submit to Stakeholder" />
                                <a href="@link" class="btn btn-dark">Close</a>
                            </div>
                        </div>
                    }
                </div>
            </div>
        }
    </div>
</section>

