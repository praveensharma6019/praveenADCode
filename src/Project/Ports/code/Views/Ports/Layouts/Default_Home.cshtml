                                                    <button type="button" id="topNavCollapse" class="btn btn-menu">
                                                        <!--<i class="fas fa-align-justify"></i>-->
                                                        <i class="fas fa-bars"></i>
                                                    </button>
                                                </div>
                                                <!-- /Toggle button to show/hide top menu -->
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 text-left"></div>
                                </div>
                            </div>
                        </div>
                        <div class="row nav-bg-blue mainmenu">
                            <div class="col-sm-8 col-md-9">
                                @Html.Sitecore().Placeholder("navbar")
                            </div>
                            <div class="col-sm-4 col-md-3 mobile-none">
                                <p class="mt-1"><a href="#cTrackingModal" data-toggle="modal" data-target="#cTrackingModal" class="btn-tracking">Container Tracking</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <section class="desktop-none bg-gray">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-5">
                        </div>
                        <div class="col-7 text-right" id="helo">
                            <p><a href="#cTrackingModal" data-toggle="modal" data-target="#cTrackingModal" class="btn-tracking">Container Tracking</a></p>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </header>


    <!-- /Header -->
    <!--  Main content Section-->
    <main role="main" class="main">
        <div class="contentSection homepage">
            @Html.Sitecore().Placeholder("page-layout")
        </div>
    </main><!-- /.container -->
    <!--  /Main content Section-->
    <!-- Footer -->
    <footer class="pt-5 bg-white position-relative d-block">
        @Html.Sitecore().Placeholder("footer")
        <div class="footerBtmPanel mt-3 py-2 d-md-block d-none">
            <div class="container">
                <div class="row row-grid align-items-end ftrBtmInner">
                    <div class="col-12 col-md-auto text-center text-md-left">

                        <div class="d-none d-md-block">© 2019 Adani Group</div>
                    </div>
                    <div class="col-12 col-md-auto text-center text-md-left ml-md-auto">
                        <div class="footerMenu">
                            <ul class="ft-policy-links">
                                <li>
                                    <a href="/privacy-policy">
                                        Privacy Policy
                                    </a>
                                </li>
                                <li>
                                    <a href="/disclaimer">
                                        Legal Disclaimer
                                    </a>
                                </li>
                                <li>
                                    <a href="/terms-and-conditions">
                                        Terms &amp; Conditions
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- The Modal -->
    <div class="modal video" id="cTrackingModal">
        <div class="modal-dialog modal-dialog-centered modal-xs">
            <div class="modal-content modal-lg">
                <button type="button" class="close" data-dismiss="modal">×</button>
                <!-- Modal body -->
                <div class="modal-body bg-white c-tracking p-4">
                    <h3 class="mb-3 txt-md">Container Tracking</h3>
                    @Html.Sitecore().Placeholder("container-tracking-form")
                </div>
            </div>
        </div>
    </div>
    <!-- /Footer -->
    <a id="back-to-top" href="#" class="back-to-top" role="button" title="Click to return on the top page" data-toggle="tooltip" data-placement="left">
        <span class="fa fa-angle-up"></span>
    </a>
    <!-- Bootstrap core JavaScript
    ================================================== -->
    @Html.Sitecore().Placeholder("page-sidebar")

    <script src="~/scripts/Ports/portshome.min.js"></script>

    <script type="text/javascript" charset="utf8" src="https://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.4/jquery.dataTables.min.js"></script>
