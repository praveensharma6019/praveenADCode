if (window.location.href.indexOf("?na=") > -1) 
{ var value = window.location.href.split("?na=")[1];
	if(value == "suspicious" || value == "Suspicious")
			 $(".sub option:contains(Suspicious)").attr('selected', 'selected');// $(".sub select").val("Suspicion");
	else if(value == "Feedback")
			 $(".sub option:contains(Feedback)").attr('selected', 'selected');// $(".sub select").val("Feedback");
}	
</script>

