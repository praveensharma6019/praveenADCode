@using Sitecore.Foundation.SitecoreExtensions.Extensions
@using Sitecore.Data.Fields
@using Sitecore.Data.Items

@{


    var datasource = Sitecore.Mvc.Presentation.RenderingContext.Current.Rendering.DataSource;
    var element = Sitecore.Context.Database.GetItem(datasource);

    Sitecore.Data.Fields.ImageField imgField = ((Sitecore.Data.Fields.ImageField)element.Fields["BackgroundImage"]);

    string imgurl = Sitecore.Resources.Media.MediaManager.GetMediaUrl(imgField.MediaItem);
}

<div class="cta_apply">
    <a href="#"><amp-img alt="" width="24" height="121" src="@imgurl"></a>
</div>
<ul class="social_icons">
    @foreach (var item in element.GetChildren().ToList())
    {
        Sitecore.Data.Fields.ImageField imgFields = ((Sitecore.Data.Fields.ImageField)item.Fields["Image"]);
        string imgurls = Sitecore.Resources.Media.MediaManager.GetMediaUrl(imgFields.MediaItem);
        Sitecore.Data.Fields.LinkField linkFields = item.Fields["Link"];
        var link = linkFields.GetFriendlyUrl();
        <li><a href="@link"><amp-img alt="" width="30" height="30" src="@imgurls"></a></li>
    }

</ul>