	<events>
		<event name="item:copied">
			<handler type="Adani.SuperApp.Airport.Foundation.SitecoreExtension.Platform.CustomEvents.RestrictItemCreation, Adani.SuperApp.Airport.Foundation.SitecoreExtension.Platform" method="OnItemCopied" />
		</event>
		<event name="item:added">
			<handler type="Adani.SuperApp.Airport.Foundation.SitecoreExtension.Platform.CustomEvents.RestrictItemCreation, Adani.SuperApp.Airport.Foundation.SitecoreExtension.Platform" method="OnItemAdded" />
		</event>
	</events>
</sitecore>