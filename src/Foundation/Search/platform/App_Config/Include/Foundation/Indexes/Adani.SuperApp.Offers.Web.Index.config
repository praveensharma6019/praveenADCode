							</field>
							<field fieldName="OfferDiscountPercentage" returnType="string">
								Adani.SuperApp.Airport.Foundation.Search.Platform.ComputedFields.OfferDiscountPercentComputedField,Adani.SuperApp.Airport.Foundation.Search.Platform
							</field>
							<field fieldName="LOBOffer" returnType="string">
								Adani.SuperApp.Airport.Foundation.Search.Platform.ComputedFields.LOBPromotion,Adani.SuperApp.Airport.Foundation.Search.Platform
							</field>
							<field fieldName="OfferRedirectionURL" returnType="string">
