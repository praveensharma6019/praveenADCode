							</field>
							<field fieldName="OfferExtraImageUrl" returnType="string">
								Adani.SuperApp.Airport.Foundation.Search.Platform.ComputedFields.OfferExtraImageComputedField,Adani.SuperApp.Airport.Foundation.Search.Platform
							</field>
							<field fieldName="OfferTCLinkText" returnType="string">
