							<field fieldName="Tab Title" returnType="string">
								Adani.SuperApp.Airport.Foundation.Search.Platform.ComputedFields.OfferTabTitleComputedField,Adani.SuperApp.Airport.Foundation.Search.Platform
							</field>
							<field fieldName="TCViewAllURL" returnType="string">
