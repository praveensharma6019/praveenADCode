							</field>
							<field fieldName="FullScreenImage" returnType="string">
								Adani.SuperApp.Airport.Foundation.Search.Platform.ComputedFields.OfferFullSizeImageComputedField,Adani.SuperApp.Airport.Foundation.Search.Platform
							</field>
							<field fieldName="Active" returnType="bool">
